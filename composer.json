{
    "name": "powerpbx/vultr-api-client",
    "description": "PHP API client for api.vultr.com",
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/powerpbx/vultr-api-client",
    "authors": [
        {
            "name": "Zach aka usefulz",
            "email": "unknown@example.com"
        },
        {
            "name": "malc0mn",
            "email": "malc0mn@advalvas.be"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "5.3 - 6.3",
        "phpunit/phpunit": ">=4.8"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use GuzzleHttpAdapter, require guzzlehttp/guzzle:^5.0|^6.0"
    },
    "autoload": {
        "psr-4": { "Vultr\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
